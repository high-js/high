{
  name: 'hello-example',
  version: '1.0.0',
  authors: 'Manuel Vila <mvila@3base.com>',
  license: 'MIT',
  repository: 'git://github.com/runtools/run',
  is: ['js/tool'],
  commands: {
    'hello': {
      implementation: './hello',
      aliases: ['hi'],
      parameters: {
        name: 'World'
      },
      options: {
        color: 'white'
      }
    },
    'hello-universe': {
      run: 'hello $0 --color=${config.color}',
      parameters: {
        name: 'Universe'
      },
      options: {
        color: 'yellow'
      }
    }
  }
}
