{
  name: 'hello-example',
  version: '1.0.0',
  authors: 'Manuel Vila <mvila@3base.com>',
  license: 'MIT',
  repository: {
    type: 'git',
    url: 'git://github.com/runtools/run'
  },
  commands: {
    'hello': {
      file: './hello',
      aliases: ['hi'],
      arguments: {
        name: 'World'
      },
      config: {
        color: 'white'
      }
    },
    'hello-universe': {
      run: 'hello $0 --color=${config.color}',
      arguments: {
        name: 'Universe'
      },
      config: {
        color: 'yellow'
      }
    }
  },
  runtime: {
    name: 'node',
    version: '>=4.3.2'
  }
}
