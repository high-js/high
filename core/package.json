{
  "name": "run-core",
  "version": "0.7.0",
  "description": "Resdir resources runner",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/runtools/run"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@resdir/version-range": "file:../../resdir/public/packages/version-range",
    "child-process-promise": "^2.2.1",
    "core-decorators": "^0.19.0",
    "deep-freeze": "^0.0.1",
    "flatnest": "^1.0.0",
    "fuse.js": "^3.0.2",
    "inquirer": "^3.0.1",
    "inquirer-autocomplete-prompt": "^0.8.0",
    "is-directory": "^0.3.1",
    "json5": "^0.5.1",
    "jsonfile": "^3.0.0",
    "koa": "^2.2.0",
    "koa-json-body": "^5.3.0",
    "lodash": "^4.17.4",
    "quote-unquote": "^1.0.0",
    "run-common": "file:../common",
    "semver": "^5.3.0",
    "shell-quote": "^1.6.1",
    "source-map-support": "^0.4.15",
    "sync-request": "^4.1.0",
    "then-request": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "jest": "^20.0.1"
  }
}
