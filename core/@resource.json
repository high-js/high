{
  "@import": "js/npm-package",
  "name": "run-core",
  "version": "0.11.7",
  "description": "Resdir resources runner",
  "author": "Manuel Vila <mvila@3base.com>",
  "repository": "https://github.com/resdir/resdir.git",
  "license": "MIT",
  "files": [
    "./dist",
    "./browser"
  ],
  "main": "./dist/index.js",
  "dependencies": {
    "deep-freeze": "^0.0.1",
    "fs-extra": "^4.0.2",
    "is-directory": "^0.3.1",
    "lodash": "^4.17.4",
    "@resdir/console": "^0.1.0",
    "@resdir/expression": "^0.1.0",
    "@resdir/file-manager": "^0.1.0",
    "@resdir/json-rpc": "^0.1.0",
    "@resdir/registry-client": "^0.1.0",
    "@resdir/resource-identifier": "^0.1.0",
    "@resdir/resource-key": "^0.1.0",
    "@resdir/resource-specifier": "^0.1.0",
    "@resdir/util": "^0.1.0",
    "@resdir/version-range": "^0.1.2"
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    },
    "tests": {
      "@import": "1place/esnext-jest",
      "@directory": "./tests"
    },
    "bundler": {
      "@import": "1place/esnext-bundler",
      "entryFile": "./src/browser.js",
      "bundleFile": "./browser/run-core.js",
      "targets": {
        "chrome": "50",
        "safari": "10",
        "firefox": "53",
        "edge": "14"
      },
      "format": "iife",
      "name": "Resource",
      "fileWatcher": {
        "@import": "tool/file-watcher",
        "files": [
          "./src/browser.js"
        ],
        "onFileModified": {
          "@run": "@parent run"
        }
      }
    }
  }
}
