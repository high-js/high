{
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./",
    "name": {
      "@type": "string"
    },
    "age": {
      "@type": "number"
    },
    "formatGreetingMethod": {
      "@type": "method",
      "@input": {
        "verb": {
          "@default": "Hello",
          "@position": 0
        },
        "shout": {
          "@default": false
        }
      }
    },
    "formatGreetingExpression": {
      "@type": "method",
      "@run": "formatGreetingMethod Hi"
    },
    "formatTags": {
      "@type": "method",
      "@input": {
        "tags": {
          "@default": [],
          "@position": 0,
          "@isVariadic": true
        }
      }
    },
    "formatNameAndAge": {
      "@type": "method",
      "@input": {
        "name": {
          "@type": "string"
        },
        "age": {
          "@type": "number",
          "@isOptional": true
        }
      }
    },
    "address": {
      "city": {
        "@type": "string"
      },
      "country": {
        "@type": "string"
      }
    },
    "hasBeenBuilt": false,
    "build": {
      "@type": "method",
      "@listen": "publish"
    },
    "publish": {
      "@type": "method",
      "@run": "@emit publish"
    },
    "hookTestResults": [],
    "hookTest": {
      "@type": "method",
      "@before": "beforeHookTest",
      "@after": "afterHookTest"
    },
    "beforeHookTest": {
      "@type": "method"
    },
    "afterHookTest": {
      "@type": "method"
    }
  }
}
