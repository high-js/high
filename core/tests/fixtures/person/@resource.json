{
  "@name": "run/person-test",
  "@version": "1.0.0",
  "@export": {
    "@runtime": "node@>=6.10.0",
    "@implementation": "./",
    "name": {"@type": "string"},
    "age": {"@type": "number"},
    "@parameters": {
      "shout": false
    },
    "formatGreetingMethod": {
      "@type": "method",
      "@parameters": {
        "verb": {"@value": "Hello", "@position": 0}
      }
    },
    "formatGreetingMacro": {
      "@type": "macro",
      "@parameters": {
        "verb": {"@value": "Hi", "@position": 0}
      },
      "@expression": "formatGreetingMethod $verb"
    },
    "address": {
      "@implementation": "./address",
      "city": {"@type": "string"},
      "country": {"@type": "string"}
    },
    "hasBeenBuilt": false,
    "build": {
      "@type": "method",
      "@listen": "before:publish"
    },
    "publish": {
      "@type": "method",
      "@emit": "*:publish"
    }
  }
}
