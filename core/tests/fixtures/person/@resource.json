{
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./",
    "name": {
      "@type": "string"
    },
    "age": {
      "@type": "number"
    },
    "formatGreetingMethod": {
      "@type": "method",
      "@input": {
        "verb": {
          "@position": 0,
          "@value": "Hello"
        },
        "shout": false
      }
    },
    "formatGreetingExpression": {
      "@type": "method",
      "@input": {
        "verb": {
          "@position": 0,
          "@value": "Hi"
        }
      },
      "@run": "formatGreetingMethod $verb"
    },
    "address": {
      "city": {
        "@type": "string"
      },
      "country": {
        "@type": "string"
      }
    },
    "hasBeenBuilt": false,
    "build": {
      "@type": "method",
      "@listen": "publish"
    },
    "publish": {
      "@type": "method",
      "@run": "@emit publish"
    },
    "hookTestResults": [],
    "hookTest": {
      "@type": "method",
      "@before": "beforeHookTest",
      "@after": "afterHookTest"
    },
    "beforeHookTest": {
      "@type": "method"
    },
    "afterHookTest": {
      "@type": "method"
    }
  }
}
