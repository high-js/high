{
  $name: 'person',
  $version: '1.0.0',
  $type: 'composite',
  $implementation: './',
  $runtime: 'node@>=6.10.0',
  name: {$type: 'string'},
  age: {$type: 'number'},
  formatGreetingMethod: {
    $type: 'method',
    $parameters: ['Hello']
  },
  formatGreetingCommand: {
    $type: 'command',
    $options: {ageLimit: {$alias: 'limit', $value: 45}}
  },
  formatGreetingMacro: {
    $type: 'macro',
    $expression: 'formatGreetingCommand --ageLimit=30'
  },
  formatWordsMethod: {
    $type: 'method',
    $parameters: [{$type: 'string'}],
    $variadic: true
  },
  formatWordsCommand: {
    $type: 'command',
    $parameters: [{$type: 'string'}],
    $variadic: true,
    $options: {capitalize: true}
  },
  formatWordsMacro: {
    $type: 'macro',
    $expression: 'formatWordsCommand blue green --capitalize=off'
  },
  address: {
    $type: 'composite',
    $implementation: './address',
    $runtime: 'node@>=6.10.0',
    city: {$type: 'string'},
    country: {$type: 'string'}
  }
}
