  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "browserify": "^14.0.0",
    "eslint": "^3.14.1",
    "eslint-config-next": "^0.1.15"
  }
