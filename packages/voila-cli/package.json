{
  "name": "voila-cli",
  "version": "0.1.0",
  "description": "The easiest way to deploy to AWS",
  "keywords": [
    "aws",
    "serverless",
    "microservice",
    "api",
    "lambda",
    "s3",
    "static",
    "website",
    "hosting",
    "javascript",
    "cloud",
    "deployment",
    "deploy"
  ],
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "voila": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mvila/voila"
  },
  "scripts": {
    "transpile": "babel src --out-dir dist",
    "prepublish": "npm run transpile",
    "lint": "eslint src"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-next"
  },
  "dependencies": {
    "child-process-promise": "^2.2.0",
    "fs-promise": "^1.0.0",
    "minimist": "^1.2.0",
    "node-version": "^1.0.0",
    "update-notifier": "^1.0.3",
    "voila-common": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.14.1",
    "eslint-config-next": "^0.1.15"
  }
}
