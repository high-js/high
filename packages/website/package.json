{
  "name": "@voila/website",
  "displayName": "Voil√† Website",
  "version": "0.1.8",
  "description": "The easiest way to deploy your website to AWS",
  "keywords": [
    "aws",
    "s3",
    "static",
    "website",
    "hosting",
    "cloud",
    "deployment",
    "deploy",
    "voila-package-handler"
  ],
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "voila-website": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/voilajs/voila"
  },
  "scripts": {
    "prepublish": "npm run transpile",
    "transpile": "babel src --out-dir dist"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@voila/common": "^0.3.11",
    "aws-sdk": "^2.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "bytes": "^2.4.0",
    "cheerio": "^0.22.0",
    "denodeify": "^1.2.1",
    "fs-promise": "^1.0.0",
    "hasha": "^2.2.0",
    "lodash.isequal": "^4.5.0",
    "mime-types": "^2.1.14",
    "minimist": "^1.2.0",
    "recursive-readdir": "^2.1.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2"
  }
}
