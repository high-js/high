{
  "name": "@voila/cli",
  "displayName": "Voil√† CLI",
  "version": "0.2.7",
  "description": "The easiest way to deploy to AWS",
  "keywords": [
    "aws",
    "serverless",
    "microservice",
    "api",
    "lambda",
    "s3",
    "static",
    "website",
    "hosting",
    "javascript",
    "cloud",
    "deployment",
    "deploy"
  ],
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "voila": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/voilajs/voila"
  },
  "scripts": {
    "transpile": "babel src --out-dir dist",
    "prepublish": "npm run transpile"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@voila/common": "^0.3.5",
    "child-process-promise": "^2.2.0",
    "fs-promise": "^1.0.0",
    "minimist": "^1.2.0",
    "node-version": "^1.0.0",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.8"
  }
}
