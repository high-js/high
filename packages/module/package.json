{
  "name": "@voila/module",
  "displayName": "Voila Module",
  "version": "0.2.4",
  "description": "The easiest way to deploy to AWS",
  "keywords": [
    "aws",
    "serverless",
    "microservice",
    "api",
    "lambda",
    "s3",
    "static",
    "website",
    "hosting",
    "javascript",
    "cloud",
    "deployment",
    "deploy"
  ],
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "voila-module": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/voilajs/voila"
  },
  "scripts": {
    "lint": "eslint src",
    "prepublish": "npm run transpile",
    "transpile": "babel src --out-dir dist"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-next"
  },
  "dependencies": {
    "@voila/common": "^0.2.1",
    "archiver": "^1.3.0",
    "aws-sdk": "^2.12.0",
    "babel-preset-env": "^1.1.8",
    "bytes": "^2.4.0",
    "fs-promise": "^1.0.0",
    "lodash.isequal": "^4.5.0",
    "minimist": "^1.2.0",
    "node-version": "^1.0.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sleep-promise": "^2.0.0",
    "stream-buffers": "^3.0.1",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "eslint-config-next": "^0.1.15"
  }
}
