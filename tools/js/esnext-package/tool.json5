{
  name: 'run/js-esnext-package',
  aliases: ['js/esnext-package', 'package', 'pkg'],
  version: '0.1.0-pre-alpha',
  extend: ['../package'], // 'js/package'
  export: {
    commands: {
      'initialize-esnext-package': {
        source: './dist/commands/initialize',
        listen: 'after:js/initialize-package',
        emit: 'js/initialize-esnext-package'
      },
      build: {
        source: './dist/commands/build',
        options: {},
        listen: ['before:publish-tool', 'before:js/publish-package'],
        emit: 'js/build-package'
      }
    },
    options: {
      sourceDir: './src',
      distributionDir: './dist'
    }
  },
  engine: 'node@>=6.10.0'
}
