{
  $type: "js/esnext-package",
  name: "run-cli",
  description: "One tool to run them all",
  author: "Manuel Vila <mvila@3base.com>",
  repository: "https://github.com/resdir/resdir.git",
  license: "MIT",
  bin: {
    run: "dist/bin/index.js"
  },
  preferGlobal: true,
  version: "0.8.7",
  dependencies: [
    "dotenv@^4.0.0",
    "json5@^0.5.1",
    "node-version@^1.0.0",
    "run-common@^0.6.0",
    "run-core@^0.9.0",
    "source-map-support@^0.4.15",
    "update-notifier@^2.1.0"
  ],
  test: {
    $type: "macro",
    $listen: "after:$test",
    $expression: "./tests run"
  }
}