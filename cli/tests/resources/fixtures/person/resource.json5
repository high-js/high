{
  $name: 'person',
  $version: '1.0.0',
  $type: 'composite',
  $implementation: './',
  $runtime: 'node@>=6.10.0',
  name: {$type: 'string'},
  age: {$type: 'number'},
  formatGreetingMethod: {
    $type: 'method',
    $parameters: ['Hello']
  },
  formatGreetingCommand: {
    $type: 'command',
    $options: {ageLimit: 45}
  },
  formatGreetingMacro: {
    $type: 'macro',
    $expression: 'formatGreetingCommand --ageLimit=30'
  },
  address: {
    $type: 'composite',
    $implementation: './address',
    $runtime: 'node@>=6.10.0',
    city: {$type: 'string'},
    country: {$type: 'string'}
  }
}
