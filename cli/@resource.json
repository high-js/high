{
  "@import": "1place/npm-package#^0.1.0",
  "name": "run-cli",
  "version": "0.11.10",
  "description": "One tool to run them all",
  "author": "Manuel Vila <hi@mvila.me>",
  "license": "MIT",
  "repository": "https://github.com/runtools/run.git",
  "bin": {
    "run": "dist/node/cjs/bin/index.js"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "json5": "^0.5.1",
    "@resdir/console": "^0.1.0",
    "run-core": "^0.11.49",
    "source-map-support": "^0.4.15"
  },
  "@unpublishable": {
    "macOSPacker": {
      "@import": "js/packer#^0.1.0",
      "entry": "./dist/node/cjs/bin/index.js",
      "output": "./builds/run-macos",
      "nodeVersion": "latest",
      "platform": "macos",
      "architecture": "x64",
      "isPublic": true,
      "reinstallDependencies": true
    },
    "linuxPacker": {
      "@import": "js/packer#^0.1.0",
      "entry": "./dist/node/cjs/bin/index.js",
      "output": "./builds/run-linux",
      "nodeVersion": "latest",
      "platform": "linux",
      "architecture": "x64",
      "isPublic": true,
      "reinstallDependencies": true
    },
    "tests": {
      "@import": "1place/jest#^0.1.0",
      "@directory": "./tests"
    }
  }
}
