{
  "name": "run-cli",
  "displayName": "âš¡run",
  "version": "0.5.4",
  "description": "One tool to run them all",
  "keywords": [
    "node",
    "tool",
    "manager",
    "cli",
    "command",
    "shell",
    "run",
    "package",
    "nodejs",
    "npm",
    "dev",
    "developer",
    "development",
    "javascript"
  ],
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "run": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/runtools/run"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "child-process-promise": "^2.2.0",
    "fuse.js": "^2.6.1",
    "inquirer": "^3.0.1",
    "inquirer-autocomplete-prompt": "^0.6.0",
    "is-directory": "^0.3.1",
    "json5": "^0.5.1",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "node-version": "^1.0.0",
    "objnest": "^3.0.9",
    "quote-unquote": "^1.0.0",
    "run-common": "^0.5.2",
    "semver": "^5.3.0",
    "shell-quote": "^1.6.1",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "prettier": "^0.22.0"
  }
}
