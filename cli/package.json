{
  "name": "@high/cli",
  "displayName": "âš¡high",
  "version": "0.5.0",
  "description": "The Node.js tool manager",
  "keywords": [
    "node",
    "tool",
    "manager",
    "cli",
    "command",
    "shell",
    "run",
    "package",
    "nodejs",
    "npm",
    "dev",
    "devoloper",
    "development",
    "javascript"
  ],
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "high": "dist/bin/index.js",
    "hi": "dist/bin/index.js",
    "h": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/high-js/high"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "child-process-promise": "^2.2.0",
    "fuse.js": "^2.6.1",
    "inquirer": "^3.0.1",
    "inquirer-autocomplete-prompt": "^0.6.0",
    "jsonfile": "^2.4.0",
    "lodash.pick": "^4.4.0",
    "minimist": "^1.2.0",
    "node-version": "^1.0.0",
    "semver": "^5.3.0",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "prettier": "^0.19.0"
  }
}
