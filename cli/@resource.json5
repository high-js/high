{
  "@import": "js/npm-package",
  name: "run-cli",
  version: "0.10.11",
  description: "One tool to run them all",
  author: "Manuel Vila <mvila@3base.com>",
  repository: "https://github.com/resdir/resdir.git",
  license: "MIT",
  files: [
    "./dist"
  ],
  main: "./dist/index.js",
  bin: {
    run: "dist/bin/index.js"
  },
  preferGlobal: true,
  dependencies: [
    "dotenv@^4.0.0",
    "json5@^0.5.1",
    "node-version@^1.0.0",
    "regenerator-runtime@^0.11.0",
    "@resdir/console@^0.1.0",
    "run-core@^0.10.0",
    "source-map-support@^0.4.15",
    "update-notifier@^2.1.0"
  ],
  "@unpublishable": {
    transpiler: {
      "@import": "resdir/esnext-transpiler"
    },
    tests: {
      "@import": "resdir/esnext-jest",
      "@directory": "./tests"
    }
  }
}