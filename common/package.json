{
  "name": "run-common",
  "version": "0.5.9",
  "description": "Miscellaneous functions used by âš¡run",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/runtools/run"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "flow-runtime",
        {
          "assert": true
        }
      ],
      "transform-flow-strip-types"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/old/"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-spinners": "^1.0.0",
    "flow-runtime": "0.11.1",
    "fs-extra": "^3.0.1",
    "js-yaml": "^3.8.4",
    "json5": "^0.5.1",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3",
    "ora": "^1.2.0",
    "semver": "^5.3.0",
    "slice-ansi": "0.0.4",
    "strict-uri-encode": "^2.0.0",
    "window-size": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "jest": "^20.0.1",
    "prettier": "^1.3.1"
  }
}
